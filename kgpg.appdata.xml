  <url type="help">http://docs.kde.org/stable/en/kdeutils/kgpg/index.html</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kgpg.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kgpg</binary>
  </provides>
</component>
